<mujoco model="Trilinear flex spheres (Gmsh)">
  <compiler meshdir="." autolimits="true"/>
  <include file="/home/mshashank02/SoftObjectsV2/objects/large_1/tet_coarse/scene.xml"/>

  <option solver="CG" tolerance="1e-6" timestep="1e-3" integrator="implicitfast"/>
  <size memory="100M"/>

  <visual>
    <map stiffness="100"/>
    <rgba bv="0 0 0 0" bvactive="0 0 0 0"/>
  </visual>

  <worldbody>
    <geom type="box" pos="1.5 0 0.25" size=".5 2 .25"/>
    <geom type="box" pos="0 0 0.25" size="2 2 .05" euler="0 15 0"/>

    <!-- Deformable (falls under gravity) -->
    <flexcomp name="soft" type="gmsh"
              file="/home/mshashank02/SoftObjectsV2/objects/large_1/tet_coarse/tetwild_out_.msh"
              dim="3" dof="trilinear"
              pos="-.5 -.5 1" scale="1 1 1" mass="5" radius="0.001">
      <elasticity young="4e4" poisson="0.49" damping="0.01"/>
      <contact selfcollide="none" internal="false"/>
    </flexcomp>

    <!-- Rigid copy (DYNAMIC: falls under gravity) -->
    <body name="rigid_body" pos="-0.5 -0.1 1.0">
      <freejoint/>
      <!-- Give it mass & inertia so physics applies.
           If you don't know the inertia, start with an estimate and tune. -->
      <inertial pos="0 0 0" mass="5" diaginertia="1e-3 1e-3 1e-3"/>
      <flexcomp name="rigid_copy" type="gmsh"
                file="/home/mshashank02/SoftObjectsV2/objects/large_1/tet_coarse/tetwild_out_.msh"
                dim="3" dof="trilinear" rigid="true"
                pos="0 0 0" scale="1 1 1" radius="0.001">
        <contact selfcollide="none" internal="false"/>
      </flexcomp>
    </body>
  </worldbody>
</mujoco>
